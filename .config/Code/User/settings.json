{
    "git.confirmSync": false,
    "git.autofetch": true,
    "git.enableCommitSigning": true,
    "workbench.preferredDarkColorTheme": "Atom One Dark",
    "workbench.preferredLightColorTheme": "Atom One Dark",
    "workbench.colorTheme": "Atom One Dark",
    "workbench.iconTheme": "vscode-icons",
    "workbench.startupEditor": "newUntitledFile",
    "shellcheck.disableVersionCheck": false,
    "shellcheck.exclude": ["SC1091", "SC1071"],
    "window.autoDetectColorScheme": true,
    "editor.fontFamily": "Fira Code, monospace",
    "editor.fontWeight": "450",
    "editor.fontLigatures": true,
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "terminal.integrated.defaultProfile.linux": "/bin/fish",
    "files.autoSave": "afterDelay",
    "settingsSync.keybindingsPerPlatform": false,
    "explorer.confirmDelete": false,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.fontSize": 16,
    "explorer.confirmDragAndDrop": false,
    "shellcheck.executablePath": "shellcheck",
    "editor.formatOnSave": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "php.validate.executablePath": "/usr/bin/php7",
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "prettier.quoteProps": "consistent",
    "prettier.singleQuote": true,
    "prettier.tabWidth": 4,
    "git.ignoreRebaseWarning": true,
    "editor.lineNumbers": "on",
    "tabnine.experimentalAutoImports": true
}
